void checkEncoderButton() {
    int btnState = digitalRead(SW);

    if (btnState == LOW) {
        if (!buttonPressed && (millis() - lastButtonPress > 50)) {
            buttonPressed = true;

                 switch (current_screen) {
                    case 0:
                        current_screen = 1;
                        break;
                    case 1:
                        current_screen = 2;

                        cuartaPantallaActive = false;
                        terceraPantallaActive = false;
                        break;
                    case 2:
                        current_screen = 0;
                        break;
                        
                    default:
                        break;
                }
        }
    } else {
        buttonPressed = false;
    }

}

