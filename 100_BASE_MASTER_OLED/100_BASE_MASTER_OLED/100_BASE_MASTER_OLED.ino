void setup() {
  Serial.begin(9600);
  u8g2.begin();

  //SWITCH
  pinMode(switchPin, INPUT_PULLUP);

  pinMode(CLK,INPUT);
  pinMode(DT,INPUT);
  pinMode(SW, INPUT_PULLUP);
  lastStateCLK = digitalRead(CLK);
  attachInterrupt(digitalPinToInterrupt(CLK), encoder, LOW);

  //RTC y RELAY
  pinMode(RELE, OUTPUT);	
  if (! rtc.begin()) {				
  Serial.println("Modulo RTC no encontrado !");	
  while (1);	
  }
}

