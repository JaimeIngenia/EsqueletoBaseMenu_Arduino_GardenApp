void setup() {
  Serial.begin(9600);
  //SWITCH

  pinMode(switchPin, INPUT_PULLUP);

  //ENCODER

  pinMode(CLK,INPUT);
  pinMode(DT,INPUT);
  pinMode(SW, INPUT_PULLUP);
  lastStateCLK = digitalRead(CLK);

  attachInterrupt(digitalPinToInterrupt(CLK), encoder, LOW);// interrupcion sobre pin A con
  //OLED

  u8g2.setColorIndex(1);  // set the color to white
  u8g2.begin();
  u8g2.setBitmapMode(1);

}

void loop() {

  Serial.println(freeMemory());

  arrancar();

  while (digitalRead(switchPin) == LOW )
  {

  //ENCOER BUTTON PRESS
  checkEncoderButton();



    item_sel_previous = item_selected - 1;
    if (item_sel_previous < 0) {item_sel_previous = NUM_ITEMS - 1;} // previous item would be below first = make it the last
    item_sel_next = item_selected + 1;  
    if (item_sel_next >= NUM_ITEMS) {item_sel_next = 0;} // next item would be after last = make it the first
      
      
    u8g2.clearBuffer();

    if (current_screen == 0) { // MENU SCREEN 
      u8g2.drawXBMP(0, 22, 128, 21, bitmap_item_sel_outline);
            // draw previous item as icon + label
        u8g2.setFont(u8g_font_7x14);
        u8g2.drawStr(25, 15, menu_items[item_sel_previous]); 
        u8g2.drawXBMP( 4, 2, 16, 16, bitmap_icons[item_sel_previous]);          

        // draw selected item as icon + label in bold font
        u8g2.setFont(u8g_font_7x14B);    
        u8g2.drawStr(25, 15+20+2, menu_items[item_selected]);   
        u8g2.drawXBMP( 4, 24, 16, 16, bitmap_icons[item_selected]);     

        // draw next item as icon + label
        u8g2.setFont(u8g_font_7x14);     
        u8g2.drawStr(25, 15+20+20+2+2, menu_items[item_sel_next]);   
        u8g2.drawXBMP( 4, 46, 16, 16, bitmap_icons[item_sel_next]);  

        // draw scrollbar background
        u8g2.drawXBMP(128-8, 0, 8, 64, bitmap_scrollbar_background);

        // draw scrollbar handle
        u8g2.drawBox(125, 64/NUM_ITEMS * item_selected, 3, 64/NUM_ITEMS); 

        // draw upir logo
        u8g2.drawXBMP(128-16-4, 64-4, 16, 4, upir_logo);  

        // Llama a nextPage() para finalizar la página actual
        u8g2.nextPage();
      } 

      else  if (current_screen == 1){

        switch(item_selected) {

          case 0: 
              primeraPantalla();
              // Llama a nextPage() para finalizar la página actual
              u8g2.nextPage();
              break;
          case 1:
              segundaPantalla();
              // Llama a nextPage() para finalizar la página actual
              u8g2.nextPage();
              break;
          case 2:
              terceraPantalla();
              // Llama a nextPage() para finalizar la página actual
              u8g2.nextPage();
              break;
          case 3: 
              cuartaPantalla();
              // Llama a nextPage() para finalizar la página actual
              u8g2.nextPage();
              break;
          default:
            break;
        }
    
      }     

       else if (current_screen == 2) { // QR SCREEN
           u8g2.drawXBMP( 0, 0, 128, 64, bitmap_qr_codes[item_selected]); // draw qr code screenshot
       }   
 
    u8g2.sendBuffer();

  }
 
}

//**************************************************************
//          FUNCIÓN ENCODER CAMBIO DE POSICIÓN MENU
//**************************************************************

void encoder() {
    static unsigned long ultimaInterrupcion = 0;
    unsigned long tiempoInterrupcion = millis();

    if (tiempoInterrupcion - ultimaInterrupcion > 5) {
        if (!cuartaPantallaActive) {
            if (digitalRead(DT) == HIGH) {
                POSICION++;
                item_selected++;
                if (item_selected >= NUM_ITEMS) {
                    item_selected = 0;
                }
            } else {
                POSICION--;
                item_selected--;
                if (item_selected < 0) {
                    item_selected = NUM_ITEMS - 1;
                }
            }
        } else {
            if (digitalRead(DT) == HIGH) {
                POSICION++;
            } else {
                POSICION--;
            }
            POSICION = min(12, max(0, POSICION)); // Limita POSICION a valores entre 0 y 12
            item_selected = 3; // Mantén item_selected en 3 mientras estés en la cuarta pantalla
        }
        ultimaInterrupcion = tiempoInterrupcion;
    }
}

